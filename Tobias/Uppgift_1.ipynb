    "# a)\n",
    "import pandas as pd\n",
    "import hashlib as hl\n",
    "# HUN \n",
    "\n",
    "df = pd.read_csv(\"athlete_events.csv\", usecols=[\"Team\", \"Name\"])\n",
    "hungary = df[df[\"Team\"] == \"Hungary\"]\n",
    "\n",
    "Hashes = hungary[\"Name\"].apply(lambda hungary: hl.sha256(hungary.encode()).hexdigest()) \n",
    "\n",
    "Hashes.head()\n",
    "\n"
   ]
